		double xa, xb, xc, ya, yb, yc;
		
		System.out.println("Enter the measure of triangle x: ");
		xa = sc.nextDouble();
		xb = sc.nextDouble();
		xc = sc.nextDouble();
		
		
		ya = sc.nextDouble();
		yb = sc.nextDouble();
		yc = sc.nextDouble();
		
		
		
		
		sc.close();

	}

}
